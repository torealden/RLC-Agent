{
  "metadata": {
    "title": "Agricultural Econometric Models Reference",
    "version": "2.0",
    "created": "2026-01-30",
    "organization": "RLC Consulting",
    "purpose": "Machine-readable reference for LLM-based commodity forecasting systems",
    "database": "rlc_commodities"
  },
  "decision_framework": {
    "description": "Use this framework to select appropriate models based on task requirements",
    "steps": [
      {
        "step": 1,
        "action": "parse_request",
        "description": "Identify commodity, forecast horizon, and output type"
      },
      {
        "step": 2,
        "action": "query_database",
        "description": "Retrieve relevant data from rlc_commodities tables"
      },
      {
        "step": 3,
        "action": "select_model",
        "description": "Choose model based on horizon and data availability"
      },
      {
        "step": 4,
        "action": "apply_equations",
        "description": "Implement model equations with retrieved parameters"
      },
      {
        "step": 5,
        "action": "generate_forecast",
        "description": "Produce point estimates and uncertainty ranges"
      },
      {
        "step": 6,
        "action": "validate",
        "description": "Check forecast against historical patterns and constraints"
      },
      {
        "step": 7,
        "action": "document",
        "description": "Record methodology, assumptions, and data sources"
      }
    ],
    "model_selection_rules": [
      {
        "condition": "horizon_days <= 90 AND task_type == 'price_forecast'",
        "recommended_models": ["lstm", "hybrid_arima_lstm", "var"],
        "rationale": "Short-term price dynamics require ML/time-series approaches"
      },
      {
        "condition": "horizon_months >= 1 AND horizon_months <= 12 AND task_type == 'balance_sheet'",
        "recommended_models": ["wasde_framework", "fapri"],
        "rationale": "Marketing year projections use balance sheet accounting"
      },
      {
        "condition": "horizon_years >= 5",
        "recommended_models": ["simple_g", "gtap", "impact"],
        "rationale": "Long-run projections require land supply and trade equilibrium models"
      },
      {
        "condition": "task_type == 'basis_forecast'",
        "recommended_models": ["basis_regression", "seasonal_adjustment"],
        "rationale": "Basis requires local supply-demand factors"
      },
      {
        "condition": "commodity_type == 'oilseed' AND task_type == 'crush_margin'",
        "recommended_models": ["joint_production", "crush_economics"],
        "rationale": "Oilseeds require joint oil/meal production modeling"
      }
    ]
  },
