  "models": {
    "balance_sheet": {
      "wasde_framework": {
        "name": "WASDE Balance Sheet Model",
        "type": "partial_equilibrium",
        "horizon": "marketing_year",
        "description": "Supply-demand accounting framework following USDA methodology",
        "core_identity": "beginning_stocks + production + imports = domestic_use + exports + ending_stocks",
        "components": {
          "production": {
            "equation": "planted_area * harvested_ratio * yield",
            "sub_models": {
              "area_response": "A_t = f(P_{t-1}, costs_t, policy_t, A_{t-1})",
              "yield_trend": "Y_t = trend + weather_deviation + technology_adjustment"
            }
          },
          "domestic_use": {
            "corn": ["feed_and_residual", "food_seed_industrial", "ethanol"],
            "soybeans": ["crush", "seed", "residual"],
            "wheat": ["food", "feed_and_residual", "seed"]
          },
          "price_determination": {
            "method": "stocks_to_use_regression",
            "equation": "ln(P) = alpha + beta * ln(stocks_to_use) + epsilon"
          }
        },
        "performance_metrics": {
          "early_season_price_rmse_pct": 18,
          "post_harvest_price_rmse_pct": 5,
          "ending_stocks_error_source": "production and export revisions drive 60-70%"
        },
        "required_queries": [
          "SELECT * FROM usda_wasde WHERE commodity = ? AND marketing_year = ?",
          "SELECT * FROM usda_psd WHERE commodity = ? ORDER BY year DESC LIMIT 10"
        ]
      }
    },
    "price_forecasting": {
      "arima": {
        "name": "ARIMA",
        "type": "time_series",
        "horizon": "short_term",
        "description": "Autoregressive Integrated Moving Average for stationary series",
        "specification": {
          "general_form": "ARIMA(p,d,q)",
          "equation": "Y_t = c + phi_1*Y_{t-1} + ... + phi_p*Y_{t-p} + theta_1*e_{t-1} + ... + theta_q*e_{t-q} + e_t",
          "typical_orders": {
            "corn_daily": [2, 1, 1],
            "soybeans_daily": [1, 1, 1],
            "wheat_daily": [2, 1, 2]
          }
        },
        "strengths": ["interpretable", "fast_computation", "good_for_stationary_series"],
        "weaknesses": ["assumes_linearity", "struggles_with_regime_changes"]
      },
      "var": {
        "name": "Vector Autoregression",
        "type": "time_series",
        "horizon": "short_to_medium_term",
        "description": "Multivariate time series capturing cross-commodity dynamics",
        "specification": {
          "equation": "Y_t = A_1*Y_{t-1} + ... + A_p*Y_{t-p} + e_t",
          "typical_variables": ["corn_price", "soybean_price", "wheat_price", "crude_oil", "usd_index"]
        },
        "applications": ["price_transmission", "spillover_analysis", "impulse_response"]
      },
      "lstm": {
        "name": "Long Short-Term Memory Neural Network",
        "type": "deep_learning",
        "horizon": "short_to_medium_term",
        "description": "Recurrent neural network capturing long-range dependencies",
        "architecture": {
          "input_layer": "price_sequence",
          "hidden_layers": {
            "lstm_units": [64, 32],
            "dropout": 0.2
          },
          "output_layer": "dense_1"
        },
        "hyperparameters": {
          "sequence_length": 60,
          "batch_size": 32,
          "epochs": 100,
          "learning_rate": 0.001
        },
        "performance": {
          "vs_arima_improvement_pct": "10-25",
          "best_for": ["nonlinear_patterns", "regime_changes", "volatility_clustering"]
        }
      },
      "hybrid_arima_lstm": {
        "name": "Hybrid ARIMA-LSTM",
        "type": "ensemble",
        "horizon": "short_to_medium_term",
        "description": "Combines linear ARIMA with nonlinear LSTM for residuals",
        "pipeline": [
          {
            "step": 1,
            "model": "arima",
            "action": "fit_and_forecast_linear_component"
          },
          {
            "step": 2,
            "model": "lstm",
            "action": "forecast_residuals_nonlinear_patterns"
          },
          {
            "step": 3,
            "action": "combine",
            "method": "additive"
          }
        ],
        "performance": {
          "vs_single_model_improvement_pct": "5-10"
        }
      },
      "vmd_eemd_lstm": {
        "name": "VMD-EEMD-LSTM Decomposition Model",
        "type": "hybrid_decomposition",
        "horizon": "short_to_medium_term",
        "description": "Multi-scale decomposition capturing different frequency dynamics",
        "pipeline": [
          {
            "step": 1,
            "method": "vmd",
            "action": "decompose_into_variational_mode_functions"
          },
          {
            "step": 2,
            "method": "eemd",
            "action": "secondary_decomposition_of_residual"
          },
          {
            "step": 3,
            "model": "lstm",
            "action": "forecast_each_component"
          },
          {
            "step": 4,
            "action": "linear_combination_of_forecasts"
          }
        ]
      },
      "transformer": {
        "name": "Transformer Architecture",
        "type": "deep_learning",
        "horizon": "medium_term",
        "description": "Attention-based architecture for time series",
        "variants": {
          "informer": "efficient_attention_for_long_sequences",
          "fedformer": "frequency_enhanced_decomposed_transformer",
          "itransformer": "inverted_transformer_for_multivariate",
          "patchtst": "patch_based_time_series_transformer"
        },
        "caution": "Mixed results on agricultural data; LSTM often outperforms on limited datasets",
        "best_for": ["long_sequences", "multivariate_forecasting"]
      }
    },
    "ensemble_methods": {
      "simple_average": {
        "description": "Equal-weighted average of model forecasts",
        "equation": "F_ensemble = (1/n) * sum(F_i)",
        "when_to_use": "When model performance is similar and uncorrelated"
      },
      "variance_weighted": {
        "description": "Weight inversely proportional to historical variance",
        "equation": "w_i = (1/var_i) / sum(1/var_j)",
        "when_to_use": "When some models have consistently lower error"
      },
      "adaptive": {
        "description": "Weights updated based on recent performance",
        "method": "exponential_smoothing_of_squared_errors",
        "when_to_use": "When relative model performance varies over time"
      }
    },
    "structural": {
      "fapri": {
        "name": "FAPRI Model",
        "type": "partial_equilibrium",
        "institution": "Food and Agricultural Policy Research Institute",
        "description": "Recursive dynamic PE system for US and global markets",
        "equations": {
          "acreage": "A_t = f(P_{t-1}, costs_t, policy_t, A_{t-1})",
          "yield": "Y_t = g(trend, inputs_t, weather_t)"
        },
        "parameters": {
          "yield_elasticity_range": [0.1, 0.3],
          "monte_carlo_iterations": 500
        },
        "documentation_url": "https://www.card.iastate.edu"
      },
      "gtap": {
        "name": "Global Trade Analysis Project",
        "type": "computable_general_equilibrium",
        "institution": "Purdue University",
        "description": "Multi-region multi-sector CGE with perfect competition",
        "coverage": {
          "regions": 140,
          "sectors": 65
        },
        "key_parameters": {
          "armington_elasticities": [2, 5],
          "land_elasticity_cet": [0.2, 0.5]
        },
        "variants": {
          "gtap_agr": "Agricultural-specific with detailed land allocation",
          "gtap_bio": "Energy-agriculture linkages for biofuel analysis"
        },
        "documentation_url": "https://gtap.agecon.purdue.edu"
      },
      "simple_g": {
        "name": "SIMPLE-G",
        "type": "partial_equilibrium_gridded",
        "description": "Simplified Model with gridded land use at 5 arcminute resolution",
        "applications": ["biofuel_policy", "land_use_change", "nutrient_pollution"],
        "hertel_equation": "q_L = [delta_D_A + delta_S_L - delta_D_L] / [1 + (eta_SI_A / eta_SE_A) + (eta_D_A / eta_SE_A)] - delta_S_L",
        "parameters": {
          "extensive_margin_elasticity": [0.05, 0.20],
          "intensive_margin_elasticity": [0.30, 0.50],
          "demand_elasticity": [-0.40, -0.20]
        }
      },
      "impact": {
        "name": "IFPRI IMPACT Model",
        "type": "partial_equilibrium",
        "institution": "International Food Policy Research Institute",
        "description": "Links economic, water, and crop sub-models",
        "coverage": {
          "regions": 320,
          "commodities": 62
        },
        "features": ["dssat_crop_integration", "stochastic_uncertainty", "water_modeling"],
        "supply_elasticity_range": [0.3, 0.6],
        "documentation_url": "https://www.ifpri.org/project/impact-model"
      }
    },
    "basis": {
      "basis_regression": {
        "name": "Basis Forecasting Model",
        "description": "Local cash-futures spread prediction",
        "equation": "Basis_t = alpha + beta_1*transport_cost + beta_2*storage_cost + beta_3*local_supply + beta_4*seasonal + epsilon",
        "components": {
          "spatial": "transportation_and_handling_costs",
          "temporal": "storage_costs_and_convenience_yield",
          "quality": "grade_differentials",
          "local_supply_demand": "elevator_capacity_utilization"
        },
        "location_factors": {
          "distance_from_river_terminal": {"coefficient": 0.015, "unit": "cents_per_mile"},
          "local_ethanol_competition": {"coefficient": -0.05, "unit": "cents_per_plant_within_50mi"},
          "harvest_premium": {"coefficient": 0.08, "unit": "cents_seasonal_adjustment"}
        }
      }
    },
