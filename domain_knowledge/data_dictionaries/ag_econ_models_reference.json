  "database_schema": {
    "usda_wasde": {
      "description": "Monthly WASDE balance sheet data",
      "key_columns": ["commodity", "marketing_year", "report_date", "beginning_stocks", "production", "imports", "domestic_use", "exports", "ending_stocks", "season_avg_price"],
      "update_frequency": "monthly",
      "query_template": "SELECT * FROM usda_wasde WHERE commodity = '{commodity}' AND marketing_year = '{year}'"
    },
    "usda_psd": {
      "description": "Production, Supply, and Distribution data",
      "key_columns": ["commodity", "country", "year", "area_harvested", "yield", "production", "imports", "exports", "ending_stocks"],
      "update_frequency": "monthly",
      "query_template": "SELECT * FROM usda_psd WHERE commodity = '{commodity}' AND country = '{country}' ORDER BY year DESC"
    },
    "census_trade": {
      "description": "US Census Bureau trade flow data",
      "key_columns": ["hs_code", "partner_country", "month", "year", "quantity", "value", "unit"],
      "update_frequency": "monthly",
      "schedule_b_codes": {
        "corn": "1005.90",
        "soybeans": "1201.90",
        "wheat": "1001",
        "soybean_oil": "1507",
        "soybean_meal": "2304.00"
      }
    },
    "cme_futures": {
      "description": "CME futures prices and settlements",
      "key_columns": ["symbol", "contract_month", "trade_date", "open", "high", "low", "settle", "volume", "open_interest"],
      "symbols": {
        "corn": "ZC",
        "soybeans": "ZS",
        "wheat": "ZW",
        "soybean_oil": "ZL",
        "soybean_meal": "ZM"
      },
      "update_frequency": "daily"
    },
    "eia_biofuels": {
      "description": "EIA biofuel production and stocks data",
      "key_columns": ["product", "date", "production", "stocks", "imports", "exports"],
      "products": ["ethanol", "biodiesel", "renewable_diesel"],
      "update_frequency": "weekly"
    },
    "usda_ams_prices": {
      "description": "USDA AMS cash market prices",
      "key_columns": ["commodity", "market", "date", "price", "basis"],
      "update_frequency": "daily"
    },
    "usda_export_sales": {
      "description": "USDA weekly export sales data",
      "key_columns": ["commodity", "marketing_year", "week_ending", "net_sales", "exports", "outstanding_sales"],
      "update_frequency": "weekly"
    },
    "usda_crop_progress": {
      "description": "Weekly crop progress and condition",
