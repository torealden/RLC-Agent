  "price_data": {
    "source": "EPA weekly RIN price reports",
    "coverage": "Separated RINs only",
    "metrics": ["Volume-weighted average price", "By D-code", "By QAP status"],
    "filters": {
      "pre_2020": {
        "D3": {"min": 0.05, "max": 3.50},
        "D4": {"min": 0.05, "max": 2.00},
        "D5": {"min": 0.05, "max": 2.00},
        "D6": {"min": 0.01, "max": 2.00}
      },
