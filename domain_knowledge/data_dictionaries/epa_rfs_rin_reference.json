  "cellulosic_waiver_credit": {
    "purpose": "Alternative compliance when cellulosic biofuel unavailable",
    "mechanism": "CWC + D4 or D5 RIN = satisfies cellulosic RVO",
    "price_formula": "MAX($0.25, $3.00 - 12_month_avg_wholesale_gasoline)",
    "market_impact": "Sets ceiling for D3 RIN prices"
  },
