  "api_query_examples": {
    "curl_examples": {
      "get_commodities": {
        "description": "List all commodity codes",
        "command": "curl -H 'X-Api-Key: YOUR_KEY' 'https://api.fas.usda.gov/api/psd/commodities'"
      },
      "get_countries": {
        "description": "List all country codes",
        "command": "curl -H 'X-Api-Key: YOUR_KEY' 'https://api.fas.usda.gov/api/psd/countries'"
      },
      "get_attributes": {
        "description": "List all attribute codes",
        "command": "curl -H 'X-Api-Key: YOUR_KEY' 'https://api.fas.usda.gov/api/psd/commodityAttributes'"
      },
      "corn_brazil_2024": {
        "description": "Get corn data for Brazil 2024",
        "command": "curl -H 'X-Api-Key: YOUR_KEY' 'https://api.fas.usda.gov/api/psd/commodity/0440000/country/BR/year/2024'"
      },
