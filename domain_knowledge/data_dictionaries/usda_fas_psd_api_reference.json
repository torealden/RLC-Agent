{
  "metadata": {
    "title": "USDA FAS Production, Supply, and Distribution (PSD) API Reference",
    "version": "1.0",
    "created": "2026-01-30",
    "author": "RLC Consulting",
    "description": "Machine-readable reference for USDA FAS PSD API including endpoints, commodity codes, country codes, attribute codes, and query construction",
    "update_frequency": "As API changes; verify codes via reference endpoints",
    "related_references": [
      {
        "name": "EPA RFS RIN Reference",
        "path": "/mnt/user-data/uploads/epa_rfs_rin_reference.json",
        "description": "EPA Renewable Fuel Standard and RIN data reference"
      },
      {
        "name": "EIA Series Reference",
        "path": "/mnt/user-data/outputs/eia_series_reference.json",
        "description": "EIA biofuel and energy production series IDs"
      }
    ]
  },
  "program_overview": {
    "name": "Production, Supply, and Distribution (PSD) Database",
    "administrator": "USDA Foreign Agricultural Service (FAS)",
    "description": "Official USDA database of global agricultural supply and demand estimates used in WASDE reports",
    "data_start_year": 1960,
    "geographic_coverage": "100+ countries and regional aggregates",
    "commodity_coverage": [
      "Grains (wheat, corn, rice, barley, sorghum, oats, rye, millet)",
      "Oilseeds (soybeans, rapeseed, sunflowerseed, peanuts, cottonseed)",
      "Oilseed products (meals, oils)",
      "Cotton",
      "Sugar",
      "Livestock (beef, pork, poultry)",
      "Dairy (milk, butter, cheese)",
      "Other (eggs, hides)"
    ],
    "update_schedule": {
      "wasde_commodities": "Monthly on WASDE release day at 12:00 PM ET",
      "horticultural": "Twice yearly",
      "export_sales": "Weekly (Thursdays 8:30 AM ET)"
    }
  },
  "api_configuration": {
    "primary_api": {
      "name": "FAS OpenData API (Recommended)",
      "base_url": "https://api.fas.usda.gov",
      "swagger_spec": "https://api.fas.usda.gov/assets/swagger/swagger.json",
      "swagger_ui": "https://apps.fas.usda.gov/opendata/swagger/ui/index",
      "interactive_docs": "https://apps.fas.usda.gov/opendatawebV2/#/home"
    },
    "legacy_api": {
      "name": "PSDOnlineDataServices (Legacy)",
      "base_url": "https://apps.fas.usda.gov/PSDOnlineDataServices",
      "swagger_ui": "https://apps.fas.usda.gov/PSDOnlineDataServices/swagger/ui/index",
      "note": "Less documented; may require precise filtering"
    },
    "alternative_endpoint": {
      "base_url": "https://apps.fas.usda.gov/OpenData/api/psd/",
      "note": "Mirrors primary API structure"
    }
  },
  "authentication": {
    "api_key_required": true,
    "registration_url": "https://api.data.gov/signup/",
    "key_format": "40-character alphanumeric string",
    "authentication_methods": [
      {
        "method": "HTTP Header (Recommended)",
        "header_name": "X-Api-Key",
        "example": "X-Api-Key: YOUR_API_KEY_HERE"
      },
      {
        "method": "Custom Header (Legacy)",
        "header_name": "API_KEY",
        "example": "API_KEY: YOUR_API_KEY_HERE"
      },
      {
        "method": "Query Parameter",
        "parameter": "api_key",
        "example": "?api_key=YOUR_API_KEY_HERE"
      }
    ],
    "rate_limits": {
      "registered_key": {
        "hourly": 1000,
        "daily": "unlimited"
      },
      "demo_key": {
        "hourly": 30,
        "daily": 50
      },
      "exceeded_response": "HTTP 429",
      "block_duration": "1 hour"
    },
    "rate_limit_headers": {
      "limit": "X-RateLimit-Limit",
      "remaining": "X-RateLimit-Remaining"
    }
  },
  "endpoints": {
    "reference_data": {
      "regions": {
        "path": "/api/psd/regions",
        "method": "GET",
        "description": "Returns all region codes and names",
        "full_url": "https://api.fas.usda.gov/api/psd/regions"
      },
      "countries": {
        "path": "/api/psd/countries",
        "method": "GET",
        "description": "Returns countries with corresponding region codes",
        "full_url": "https://api.fas.usda.gov/api/psd/countries"
      },
      "commodities": {
        "path": "/api/psd/commodities",
        "method": "GET",
        "description": "Returns all commodities with 7-digit codes",
        "full_url": "https://api.fas.usda.gov/api/psd/commodities"
      },
      "units_of_measure": {
        "path": "/api/psd/unitsOfMeasure",
        "method": "GET",
        "description": "Returns unit of measure codes and descriptions",
        "full_url": "https://api.fas.usda.gov/api/psd/unitsOfMeasure"
      },
      "commodity_attributes": {
        "path": "/api/psd/commodityAttributes",
        "method": "GET",
        "description": "Returns all attribute codes (production, supply, distribution)",
        "full_url": "https://api.fas.usda.gov/api/psd/commodityAttributes"
      }
    },
    "data_retrieval": {
      "commodity_country_year": {
        "path": "/api/psd/commodity/{commodityCode}/country/{countryCode}/year/{marketYear}",
        "method": "GET",
        "description": "Forecast data for specific commodity, country, and year",
        "parameters": {
          "commodityCode": "7-digit commodity code",
          "countryCode": "2-letter or 4-digit country code",
          "marketYear": "4-digit year (e.g., 2024)"
        }
      },
      "commodity_all_countries": {
        "path": "/api/psd/commodity/{commodityCode}/country/all/year/{marketYear}",
        "method": "GET",
        "description": "Forecast data for all countries for a commodity",
        "parameters": {
          "commodityCode": "7-digit commodity code",
          "marketYear": "4-digit year"
        }
      },
      "commodity_world": {
        "path": "/api/psd/commodity/{commodityCode}/world/year/{marketYear}",
        "method": "GET",
        "description": "World-aggregated data for a commodity",
        "parameters": {
          "commodityCode": "7-digit commodity code",
          "marketYear": "4-digit year"
        }
      },
      "data_release_dates": {
        "path": "/api/psd/commodity/{commodityCode}/dataReleaseDates",
        "method": "GET",
        "description": "Last release dates for a commodity",
        "parameters": {
          "commodityCode": "7-digit commodity code"
        }
      }
    },
    "legacy_endpoint": {
      "get_all_data": {
        "path": "/api/CommodityData/GetAllData",
        "method": "GET",
        "description": "Bulk data retrieval (requires filters)",
        "base_url": "https://apps.fas.usda.gov/PSDOnlineDataServices"
      }
    }
  },
