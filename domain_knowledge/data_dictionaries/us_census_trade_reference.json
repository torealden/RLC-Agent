{
  "metadata": {
    "title": "US Census Bureau International Trade Data Reference",
    "description": "US-specific trade codes (Schedule B for exports, HTS for imports) and Census Bureau API documentation",
    "version": "1.0",
    "created": "2026-01-28",
    "author": "RLC Consulting",
    "country": "US",
    "scope": "US export and import trade data via Census Bureau API",
    "related_references": [
      {
        "name": "International HS Codes Reference",
        "path": "hs_codes_international.json",
        "description": "Universal 6-digit HS codes (base for all country-specific extensions)"
      }
    ]
  },
  "code_systems": {
    "schedule_b": {
      "name": "Schedule B",
      "full_name": "Schedule B - Statistical Classification of Domestic and Foreign Commodities Exported from the United States",
      "administrator": "US Census Bureau",
      "use": "US EXPORTS only",
      "format": "10-digit",
      "structure": "First 6 digits = International HS code, Last 4 digits = US-specific",
      "api_parameter": "E_COMMODITY",
      "notes": "Required for Electronic Export Information (EEI) filing"
    },
    "hts": {
      "name": "HTS",
      "full_name": "Harmonized Tariff Schedule of the United States",
      "administrator": "US International Trade Commission (USITC)",
      "use": "US IMPORTS only",
      "format": "10-digit",
      "structure": "First 6 digits = International HS code, Last 4 digits = US-specific tariff classification",
      "api_parameter": "I_COMMODITY",
      "notes": "Used for customs duty determination"
    }
  },
  "api": {
    "base_url": "https://api.census.gov/data/timeseries/intltrade",
    "documentation": "https://www.census.gov/foreign-trade/reference/guides/Guide_to_International_Trade_Datasets.pdf",
    "registration": "https://api.census.gov/data/key_signup.html",
    "rate_limit": "500 calls/day without API key, unlimited with key",
    "data_format": "JSON",
    "data_start": "2013-01",
    "update_schedule": "Monthly, released with FT-900 report (typically 5th business day of month for 2-month prior data)",
    "endpoints": {
      "exports": {
        "hs": {
          "url": "https://api.census.gov/data/timeseries/intltrade/exports/hs",
          "description": "Monthly US exports by Schedule B (HS) code",
          "detail_levels": ["HS2", "HS4", "HS6", "HS10"],
          "geography": "District, Country"
        },
        "statehs": {
          "url": "https://api.census.gov/data/timeseries/intltrade/exports/statehs",
          "description": "Monthly US exports by state and HS code",
          "detail_levels": ["HS2", "HS4", "HS6"],
          "geography": "State, Country",
          "note": "HS10 not available due to confidentiality"
        },
        "porths": {
          "url": "https://api.census.gov/data/timeseries/intltrade/exports/porths",
          "description": "Monthly US exports by port and HS code",
          "detail_levels": ["HS2", "HS4", "HS6"],
          "geography": "Port, Country"
        },
        "naics": {
          "url": "https://api.census.gov/data/timeseries/intltrade/exports/naics",
          "description": "Monthly US exports by NAICS code",
          "detail_levels": ["NA2", "NA3", "NA4", "NA5", "NA6"],
          "geography": "District, Country"
        },
        "enduse": {
          "url": "https://api.census.gov/data/timeseries/intltrade/exports/enduse",
          "description": "Monthly US exports by end-use code",
          "detail_levels": ["EU1", "EU5"],
          "geography": "District, Country"
        },
        "sitc": {
          "url": "https://api.census.gov/data/timeseries/intltrade/exports/sitc",
          "description": "Monthly US exports by SITC code",
          "geography": "District, Country"
        },
        "usda": {
          "url": "https://api.census.gov/data/timeseries/intltrade/exports/usda",
          "description": "Monthly US exports by USDA agricultural/non-agricultural code",
          "geography": "District, Country"
        }
      },
      "imports": {
        "hs": {
          "url": "https://api.census.gov/data/timeseries/intltrade/imports/hs",
          "description": "Monthly US imports by HTS code",
          "detail_levels": ["HS2", "HS4", "HS6", "HS10"],
          "geography": "District, Country"
        },
        "statehs": {
          "url": "https://api.census.gov/data/timeseries/intltrade/imports/statehs",
          "description": "Monthly US imports by state of destination and HTS code",
          "detail_levels": ["HS2", "HS4", "HS6"],
          "geography": "State, Country"
        },
        "porths": {
          "url": "https://api.census.gov/data/timeseries/intltrade/imports/porths",
          "description": "Monthly US imports by port and HTS code",
          "detail_levels": ["HS2", "HS4", "HS6"],
          "geography": "Port, Country"
        },
        "naics": {
          "url": "https://api.census.gov/data/timeseries/intltrade/imports/naics",
          "description": "Monthly US imports by NAICS code",
          "detail_levels": ["NA2", "NA3", "NA4", "NA5", "NA6"],
          "geography": "District, Country"
        },
        "enduse": {
          "url": "https://api.census.gov/data/timeseries/intltrade/imports/enduse",
          "description": "Monthly US imports by end-use code",
          "detail_levels": ["EU1", "EU5"],
          "geography": "District, Country"
        },
        "sitc": {
          "url": "https://api.census.gov/data/timeseries/intltrade/imports/sitc",
          "description": "Monthly US imports by SITC code",
          "geography": "District, Country"
        },
        "usda": {
          "url": "https://api.census.gov/data/timeseries/intltrade/imports/usda",
          "description": "Monthly US imports by USDA code",
          "geography": "District, Country"
        }
      }
    },
    "key_parameters": {
      "time": {
        "description": "Time period in YYYY-MM format",
        "examples": ["2024-01", "from+2023-01+to+2023-12", "from+2024-01"]
      },
      "YEAR": {
        "description": "4-digit year (alternative to time)",
        "examples": ["2024"]
      },
      "MONTH": {
        "description": "2-digit month (use with YEAR)",
        "examples": ["01", "12"]
      },
      "E_COMMODITY": {
        "description": "Schedule B export commodity code",
        "examples": ["1005902020", "1201900090", "22*"]
      },
      "I_COMMODITY": {
        "description": "HTS import commodity code",
        "examples": ["1005902020", "1201900090"]
      },
      "CTY_CODE": {
        "description": "4-digit country code",
        "examples": ["1220", "2010", "5700"]
      },
      "COMM_LVL": {
        "description": "Commodity aggregation level",
        "values": ["HS2", "HS4", "HS6", "HS10", "NA2", "NA3", "NA4", "NA5", "NA6", "EU1", "EU5", "MAN"]
      },
      "SUMMARY_LVL": {
        "description": "Detail vs country grouping",
        "values": {
          "DET": "Individual country detail",
          "CGP": "Country groupings only"
        }
      },
      "DISTRICT": {
        "description": "2-digit customs district code"
      },
      "PORT": {
        "description": "4-digit port code"
      },
      "STATE": {
        "description": "2-letter state code"
      },
      "DF": {
        "description": "Domestic/Foreign indicator (exports only)",
        "values": {
          "1": "Domestic exports",
          "2": "Foreign re-exports"
        }
      }
    },
    "value_parameters": {
      "exports": {
        "ALL_VAL_MO": "Total export value (monthly)",
        "ALL_VAL_YR": "Total export value (year-to-date)",
        "QTY_1_MO": "Quantity 1 (monthly)",
        "QTY_1_YR": "Quantity 1 (year-to-date)",
        "QTY_2_MO": "Quantity 2 (monthly, if applicable)",
        "AIR_VAL_MO": "Air shipment value",
        "VES_VAL_MO": "Vessel shipment value",
        "CNT_VAL_MO": "Containerized vessel value",
        "AIR_WGT_MO": "Air shipping weight",
        "VES_WGT_MO": "Vessel shipping weight"
      },
      "imports": {
        "GEN_VAL_MO": "General imports value (monthly)",
        "GEN_VAL_YR": "General imports value (year-to-date)",
        "CON_VAL_MO": "Consumption imports value (monthly)",
        "CON_VAL_YR": "Consumption imports value (year-to-date)",
        "GEN_QY1_MO": "General imports quantity 1",
        "CON_QY1_MO": "Consumption imports quantity 1",
        "GEN_CIF_MO": "General imports CIF value",
        "CAL_DUT_MO": "Calculated duty",
        "DUT_VAL_MO": "Dutiable value",
        "AIR_VAL_MO": "Air shipment value",
        "VES_VAL_MO": "Vessel shipment value"
      }
    },
    "example_calls": {
      "total_exports_by_country": "https://api.census.gov/data/timeseries/intltrade/exports/hs?get=CTY_CODE,CTY_NAME,ALL_VAL_MO&time=2024-01",
      "soybean_exports_to_china": "https://api.census.gov/data/timeseries/intltrade/exports/hs?get=E_COMMODITY,ALL_VAL_MO,QTY_1_MO&time=2024-01&E_COMMODITY=1201900090&CTY_CODE=5700",
      "corn_exports_all_countries": "https://api.census.gov/data/timeseries/intltrade/exports/hs?get=CTY_CODE,CTY_NAME,ALL_VAL_MO,QTY_1_MO&time=2024-01&E_COMMODITY=1005902020",
      "ethanol_imports_by_country": "https://api.census.gov/data/timeseries/intltrade/imports/hs?get=CTY_CODE,CTY_NAME,GEN_VAL_MO,GEN_QY1_MO&time=2024-01&I_COMMODITY=2207200010",
      "all_chapter_12_exports": "https://api.census.gov/data/timeseries/intltrade/exports/hs?get=E_COMMODITY,ALL_VAL_MO&time=2024-01&E_COMMODITY=12*&COMM_LVL=HS4",
      "time_series_soybeans": "https://api.census.gov/data/timeseries/intltrade/exports/hs?get=ALL_VAL_MO,QTY_1_MO&E_COMMODITY=1201900090&CTY_CODE=5700&time=from+2023-01+to+2023-12"
    },
    "notes": {
      "missing_values": "Zero values may be missing data. Use FLAG parameters (e.g., QTY_1_MO_FLAG) to check - 'M' indicates missing",
      "wildcards": "Use * for wildcard matching (e.g., E_COMMODITY=12* for all chapter 12)",
      "cardinality": "Break large queries into smaller chunks to avoid timeouts",
      "special_characters": "Use hexadecimal equivalents for special characters (e.g., %2B for +)",
      "confidentiality": "HS10, NA5, NA6 not available in state/port endpoints due to disclosure restrictions"
    }
  },
