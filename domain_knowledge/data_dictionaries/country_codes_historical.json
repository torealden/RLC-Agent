  "census_special_codes": {
    "obsolete_marker": {
      "symbol": "(!)",
      "meaning": "Country code is obsolete/historical",
      "handling": "Map to successor country if trade date is after dissolution date; otherwise keep historical"
    },
    "summarization_marker": {
      "symbol": "(*)",
      "meaning": "Value represents a summarization including component/predecessor countries",
      "handling": "Use for historical totals; do not double-count with individual successor countries"
    },
