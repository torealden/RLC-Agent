{
  "metadata": {
    "title": "EPA RFS Data File URLs",
    "description": "URLs for EPA EMTS public data CSV files",
    "source": "https://www.epa.gov/fuels-registration-reporting-and-compliance-help/public-data-renewable-fuel-standard",
    "last_updated": "2026-01-28",
    "update_notes": "Complete set of EPA RFS files downloaded for Dec 2025"
  },
  "url_patterns": {
    "generation_breakout": {
      "description": "Annual RIN generation by D-code and producer type (domestic/importer/foreign)",
      "pattern": "https://www.epa.gov/system/files/other-files/{YYYY}-{MM}/generationbreakout_{mon}{data_year}.csv",
      "example": "https://www.epa.gov/system/files/other-files/2026-01/generationbreakout_dec2025.csv",
      "columns": ["RIN_YEAR", "Fuel Code", "DOMESTIC", "IMPORTER", "Foreign Generation", "Total RINs"],
      "notes": "Published monthly, ~30-60 days after data month. Annual totals by D-code."
    },
    "rin_data_monthly": {
      "description": "Monthly RIN generation by D-code - provides time series granularity",
      "pattern": "https://www.epa.gov/system/files/other-files/{YYYY}-{MM}/rindata_{mon}{data_year}.csv",
      "example": "https://www.epa.gov/system/files/other-files/2026-01/rindata_dec2025.csv",
      "columns": ["FUEL_CODE", "RIN_YEAR", "Production Month", "RIN_QUANTITY", "BATCH_VOLUME"],
      "notes": "Monthly data starting July 2010. Essential for seasonal analysis."
    },
    "fuel_production": {
      "description": "Fuel production by specific fuel type and category",
      "pattern": "https://www.epa.gov/system/files/other-files/{YYYY}-{MM}/fuelproduction_{mon}{data_year}.csv",
      "example": "https://www.epa.gov/system/files/other-files/2026-01/fuelproduction_dec2025.csv",
      "columns": ["RIN Year", "Fuel", "Fuel Code", "Fuel Category", "Fuel Category Code", "RIN Quantity", "Batch Volume"],
      "notes": "Detailed breakdown by fuel type (Naphtha, LPG, Biodiesel, RD, etc.)"
    },
    "available_rins": {
      "description": "Available RINs inventory by year, D-code, and assignment status",
      "pattern": "https://www.epa.gov/system/files/other-files/{YYYY}-{MM}/availablerins_{mon}{data_year}.csv",
      "example": "https://www.epa.gov/system/files/other-files/2026-01/availablerins_dec2025.csv",
      "columns": ["RIN_YEAR", "FUEL_CD", "ASSIGNMENT", "Total Generated", "Total Retired", "Total Locked", "Total Available"],
      "notes": "Shows separated vs assigned RINs. Key for supply analysis."
    },
    "retirement_transactions": {
      "description": "RIN retirements by reason code",
      "pattern": "https://www.epa.gov/system/files/other-files/{YYYY}-{MM}/retiretransaction_{mon}{data_year}.csv",
      "example": "https://www.epa.gov/system/files/other-files/2026-01/retiretransaction_dec2025.csv",
      "columns": ["RIN_YEAR", "FUEL_CD", "FUEL_CD_DESCRIPTION", "RETIRE_REASON_CD", "RETIRE_REASON_CD_DESCRIPTION", "RIN_QUANTITY"],
      "notes": "Shows compliance (code 80), invalid RINs (50), enforcement (70), etc."
    },
    "separation_transactions": {
      "description": "RIN separations by reason code",
      "pattern": "https://www.epa.gov/system/files/other-files/{YYYY}-{MM}/separatetransaction_{mon}{data_year}.csv",
      "example": "https://www.epa.gov/system/files/other-files/2026-01/separatetransaction_dec2025.csv",
      "columns": ["RIN Year", "Fuel Cd", "Fuel Cd Description", "Sep Reason Cd", "Separate Reason Cd Description", "RIN Quantity"],
      "notes": "Shows separation by obligated party (10), blending (20), export (50), etc."
    }
  },
