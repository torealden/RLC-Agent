{
  "schedules": {
    "usda_nass": {
      "name": "USDA NASS Crop Data",
      "collector": "usda_nass_collector.py",
      "frequency": "weekly",
      "day": "monday",
      "time": "16:30",
      "timezone": "America/New_York",
      "description": "Crop progress, condition, acreage, production, stocks",
      "data_types": ["crop_progress", "condition", "acreage", "production", "stocks"],
      "enabled": true
    },
    "eia_ethanol": {
      "name": "EIA Ethanol Weekly",
      "collector": "eia_ethanol_collector.py",
      "frequency": "weekly",
      "day": "wednesday",
      "time": "11:00",
      "timezone": "America/New_York",
      "description": "Weekly ethanol production, stocks, blending",
      "enabled": true
    },
    "census_trade": {
      "name": "US Census Trade Data",
      "collector": "census_trade_collector.py",
      "frequency": "monthly",
      "trigger": "release_calendar",
      "release_calendar": "config/census_release_calendar.json",
      "release_report": "FT900",
      "time": "09:00",
      "timezone": "America/New_York",
      "description": "Monthly US agricultural imports/exports by HS code. Triggered by FT900 release dates.",
      "hs_codes": {
        "grains": ["1005902030", "1001992055"],
        "soybeans": ["1201900095", "1201900005"],
        "soybean_meal": {
          "current": ["2304000010", "2304000090", "2302500000", "1208100090", "1208100010"],
          "legacy": ["2304000000"],
          "notes": "2304000000 split into 2304000010 (organic) and 2304000090 (NES) in 2023"
        },
        "soybean_oil": ["1507904050", "1507100000"],
        "cottonseed": ["1207210000", "1207290000"],
        "cottonseed_oil": ["1512210000", "1512290020", "1512290040"],
        "cottonseed_meal": ["2306100000"],
        "other": ["1205100000", "2303300000"]
      },
      "post_collection": ["bronze_to_silver_transform", "verify_and_notify"],
      "enabled": true
    },
    "cftc_cot": {
      "name": "CFTC COT Positioning",
      "collector": "cftc_cot_collector.py",
      "frequency": "weekly",
      "day": "friday",
      "time": "16:00",
      "timezone": "America/New_York",
      "description": "Weekly managed money positioning for ag futures",
      "contracts": ["corn", "soybeans", "wheat_srw", "wheat_hrw", "soybean_oil", "soybean_meal"],
      "enabled": true
    },
    "usda_fas": {
      "name": "USDA FAS Export Sales",
      "collector": "usda_fas_collector.py",
      "frequency": "weekly",
      "day": "thursday",
      "time": "08:45",
      "timezone": "America/New_York",
      "description": "Weekly export sales and shipments",
      "requires_api_key": "FAS_API_KEY",
      "enabled": false,
      "notes": "Waiting for USDA FAS API to be restored (currently returning 500 errors)"
    },
    "usda_ams_prices": {
      "name": "USDA AMS Cash Prices",
      "collector": "usda_ams_collector.py",
      "frequency": "daily",
      "time": "17:00",
      "timezone": "America/Chicago",
      "description": "Daily cash bid prices from market reports",
      "enabled": true
    },
    "weather": {
      "name": "Agricultural Weather",
      "collector": "weather_collector_agent.py",
      "frequency": "hourly",
      "description": "Weather observations for key ag regions",
      "enabled": true
    }
  },
  "metadata": {
    "created": "2026-01-28",
    "version": "1.0",
    "notes": "Initial schedule configuration for working collectors"
  }
}
