# =============================================================================
# RLC MASTER CREDENTIALS TEMPLATE
# Round Lakes Commodities - All API Keys and Secrets
# =============================================================================
# Copy this file to credentials.env and fill in your values
# Location: RLC-Agent/config/credentials.env
# =============================================================================

# -----------------------------------------------------------------------------
# DATABASE - PostgreSQL (Primary)
# -----------------------------------------------------------------------------
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=rlc_commodities
DB_USER=postgres
DB_PASSWORD=your_password_here

# -----------------------------------------------------------------------------
# USDA APIs
# -----------------------------------------------------------------------------
# USDA AMS Market News API (https://mymarketnews.ams.usda.gov/mymarketnews-api)
USDA_AMS_API_KEY=your_key_here
USDA_AMS_USERNAME=your_email@example.com

# USDA NASS Quick Stats API (https://quickstats.nass.usda.gov/api)
QUICK_STATS_API_KEY=your_key_here

# -----------------------------------------------------------------------------
# OTHER DATA APIS
# -----------------------------------------------------------------------------
# US Census Bureau (https://api.census.gov/data/key_signup.html)
CENSUS_API_KEY=your_key_here

# OpenWeatherMap (https://openweathermap.org/api)
WEATHER_API_KEY=your_key_here

# EIA - Energy Information Administration (https://www.eia.gov/opendata/)
EIA_API_KEY=your_key_here

# Eurostat - No API key required (public access)
EUROSTAT_ENABLED=true
EUROSTAT_BASE_URL=https://ec.europa.eu/eurostat/api/dissemination
EUROSTAT_LANG=EN
EUROSTAT_FORMAT=JSON

# -----------------------------------------------------------------------------
# FUTURES DATA - INTERACTIVE BROKERS (Primary)
# -----------------------------------------------------------------------------
# Download Client Portal Gateway: https://www.interactivebrokers.com/en/trading/ib-api.php
IBKR_ACCOUNT_ID=your_account_id
IBKR_GATEWAY_HOST=localhost
IBKR_GATEWAY_PORT=5000

# -----------------------------------------------------------------------------
# FUTURES DATA - TRADESTATION (Backup)
# -----------------------------------------------------------------------------
# Register app at: https://developer.tradestation.com
TRADESTATION_CLIENT_ID=your_client_id
TRADESTATION_CLIENT_SECRET=your_secret
TRADESTATION_REFRESH_TOKEN=your_token

# -----------------------------------------------------------------------------
# LLM CONFIGURATION
# -----------------------------------------------------------------------------
LLM_PROVIDER=ollama

# Ollama settings (local LLM)
OLLAMA_MODEL=llama2:13b
OLLAMA_BASE_URL=http://localhost:11434

# OpenAI settings (cloud LLM)
OPENAI_API_KEY=your_key_here
OPENAI_MODEL=gpt-4

# LLM behavior
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2000

# -----------------------------------------------------------------------------
# CLOUD STORAGE - DROPBOX
# -----------------------------------------------------------------------------
DROPBOX_ACCESS_TOKEN=your_token_here

# -----------------------------------------------------------------------------
# NOTION INTEGRATION
# -----------------------------------------------------------------------------
NOTION_API_KEY=your_key_here
NOTION_TASKS_DB=
NOTION_MEMORY_DB=
NOTION_INTERACTIONS_DB=
NOTION_WIKI_DB=

# -----------------------------------------------------------------------------
# GOOGLE API CONFIGURATION
# -----------------------------------------------------------------------------
GOOGLE_CREDENTIALS_DIR=./config
GMAIL_WORK_CREDENTIALS=gmail_work_credentials.json
GMAIL_PERSONAL_CREDENTIALS=gmail_personal_credentials.json
CALENDAR_CREDENTIALS=calendar_credentials.json
GOOGLE_TOKEN_DIR=./config/tokens

# -----------------------------------------------------------------------------
# EMAIL NOTIFICATIONS (SMTP)
# -----------------------------------------------------------------------------
ALERT_EMAIL=your_email@example.com
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@example.com
SMTP_PASSWORD=your_app_password

# -----------------------------------------------------------------------------
# USER CONFIGURATION
# -----------------------------------------------------------------------------
USER_NAME=Your Name
BUSINESS_NAME=Your Business
USER_EMAIL=your_email@example.com
USER_TIMEZONE=America/New_York
WORKING_HOURS_START=8
WORKING_HOURS_END=18

# -----------------------------------------------------------------------------
# AGENT BEHAVIOR
# -----------------------------------------------------------------------------
AUTONOMY_LEVEL=1
REQUIRE_CALENDAR_APPROVAL=false
REQUIRE_EMAIL_APPROVAL=true

# -----------------------------------------------------------------------------
# API SETTINGS
# -----------------------------------------------------------------------------
REQUEST_TIMEOUT=30
MAX_RETRIES=3
RETRY_BACKOFF=2.0

# -----------------------------------------------------------------------------
# LOGGING
# -----------------------------------------------------------------------------
LOG_LEVEL=INFO
LOG_FILE=logs/data_pipeline.log
